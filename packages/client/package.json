{
  "name": "cabasvert-client",
  "version": "0.3.0-beta.6",
  "author": "Cabas Vert",
  "description": "Cabas Vert: A vegetable basket manager app",
  "homepage": "https://github.com/cabasvert",
  "scripts": {
    "postinstall": "node bin/patch-angular.js",
    "start": "npm run prepare && ng serve",
    "build": "npm run prepare && ng build",
    "test": "npm run prepare && ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prepare": "node bin/set-versions.js",
    "license-check": "license-check",
    "build-debug": "npm run prepare && ng run app:build:debug",
    "build-production": "npm run prepare && ng run app:build:production",
    "build-android-debug": "npm run build-debug && cap sync && (cd android && ./gradlew assembleDebug)",
    "install-android-debug": "npm run build-android-debug && adb -d uninstall org.cabasvert.client && adb -d install android/app/build/outputs/apk/debug/app-debug.apk",
    "build-android-release": "npm run build-production && cap sync && (cd android && ./gradlew assembleRelease)",
    "install-android-release": "npm run build-android-release && adb -d uninstall org.cabasvert.client && adb -d install android/app/build/outputs/apk/release/app-release.apk",
    "release": "standard-version",
    "publish-release": "npm run build-production && node bin/publish-release.js"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^6.1.9",
    "@angular/core": "^6.1.9",
    "@angular/forms": "^6.1.9",
    "@angular/http": "^6.1.9",
    "@angular/platform-browser": "^6.1.9",
    "@angular/platform-browser-dynamic": "^6.1.9",
    "@angular/pwa": "^0.8.3",
    "@angular/router": "^6.1.9",
    "@angular/service-worker": "^6.1.9",
    "@capacitor/android": "^1.0.0-beta.8",
    "@capacitor/cli": "^1.0.0-beta.8",
    "@capacitor/core": "^1.0.0-beta.8",
    "@ionic-native/core": "^5.0.0-beta.21",
    "@ionic-native/secure-storage": "^5.0.0-beta.21",
    "@ionic/angular": "^4.0.0-beta.12",
    "@ionic/core": "^4.0.0-beta.12",
    "@ionic/ng-toolkit": "^1.0.8",
    "@ionic/schematics-angular": "^1.0.6",
    "@ngx-translate/core": "^10.0.2",
    "@ngx-translate/http-loader": "^3.0.1",
    "@types/pouchdb-core": "^6.4.2",
    "@types/pouchdb-find": "^6.3.3",
    "@types/pouchdb-replication": "^6.4.0",
    "cordova-plugin-secure-storage": "^2.6.8",
    "core-js": "^2.5.7",
    "hammerjs": "^2.0.8",
    "pouchdb-adapter-http": "^6.4.1",
    "pouchdb-adapter-idb": "^6.4.1",
    "pouchdb-authentication": "^1.1.0",
    "pouchdb-core": "^6.4.1",
    "pouchdb-find": "^6.4.1",
    "pouchdb-replication": "^6.4.1",
    "rxjs": "^6.3.2",
    "standard-version": "^4.4.0",
    "uid-generator": "^1.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/architect": "~0.8.3",
    "@angular-devkit/build-angular": "~0.8.3",
    "@angular-devkit/core": "~0.8.3",
    "@angular-devkit/schematics": "~0.8.3",
    "@angular/cli": "~6.2.3",
    "@angular/compiler": "~6.1.9",
    "@angular/compiler-cli": "~6.1.9",
    "@angular/language-service": "~6.1.9",
    "@ionic/lab": "~1.0.9",
    "@octokit/rest": "^15.12.1",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^10.9.4",
    "codelyzer": "~4.4.4",
    "execa": "^1.0.0",
    "fs-extra": "^7.0.0",
    "jasmine-core": "~3.2.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^1.3.1",
    "license-check": "^1.1.5",
    "protractor": "^5.4.1",
    "ts-node": "~7.0.1",
    "tslint": "~5.11.0",
    "typescript": "~2.9.2"
  },
  "license-check-config": {
    "src": [
      "src/**/*.js",
      "src/**/*.ts",
      "e2e/**/*.js",
      "e2e/**/*.ts",
      "!./node_modules/**/*"
    ],
    "path": "assets/header.txt",
    "blocking": false,
    "logInfo": false,
    "logError": true
  }
}
