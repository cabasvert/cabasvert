{
  "name": "@cabasvert/client",
  "version": "0.3.0-beta.20",
  "author": "Cabas Vert",
  "description": "Cabas Vert: A vegetable basket manager app",
  "license": "GPL-3.0",
  "homepage": "https://github.com/cabasvert",
  "private": true,
  "workspaces": {
    "nohoist": [
      "jest-preset-angular"
    ]
  },
  "scripts": {
    "postinstall": "scripts/patch-angular.js",
    "clean": "rm -rf coverage www artifacts",
    "start": "scripts/start.js",
    "start:watch": "scripts/start.js --watch",
    "start:prod": "scripts/start.js --prod",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build": "scripts/build.js all --prod",
    "build-browser-release": "scripts/build.js browser --prod",
    "build-android-debug": "scripts/build.js android --debug",
    "build-android-release": "scripts/build.js android --prod",
    "build-ios-debug": "scripts/build.js ios --debug",
    "build-ios-release": "scripts/build.js ios --prod",
    "deploy-android-debug": "adb -d uninstall org.cabasvert.client ; adb -d install android/app/build/outputs/apk/debug/app-debug.apk",
    "deploy-android-release": "adb -d uninstall org.cabasvert.client ; adb -d install android/app/build/outputs/apk/release/app-release.apk",
    "lint": "ng lint",
    "license-check": "node ../../scripts/license-check.js"
  },
  "dependencies": {
    "@angular/common": "^7.2.1",
    "@angular/core": "^7.2.1",
    "@angular/forms": "^7.2.1",
    "@angular/http": "^7.2.1",
    "@angular/platform-browser": "^7.2.1",
    "@angular/platform-browser-dynamic": "^7.2.1",
    "@angular/router": "^7.2.1",
    "@angular/service-worker": "^7.2.1",
    "@cabasvert/data": "^0.1.0-beta.4",
    "@capacitor/android": "^1.0.0-beta.13",
    "@capacitor/cli": "^1.0.0-beta.13",
    "@capacitor/core": "^1.0.0-beta.13",
    "@capacitor/ios": "^1.0.0-beta.13",
    "@ionic-native/core": "^5.0.0-beta.23",
    "@ionic-native/secure-storage": "^5.0.0-beta.23",
    "@ionic/angular": "^4.0.0-rc.2",
    "@ionic/core": "^4.0.0-rc.2",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "@types/pouchdb-core": "^7.0.1",
    "@types/pouchdb-find": "^6.3.3",
    "@types/pouchdb-replication": "^6.4.0",
    "cordova-plugin-secure-storage": "^3.0.0",
    "core-js": "^2.5.7",
    "hammerjs": "^2.0.8",
    "pouchdb-adapter-http": "^7.0.0",
    "pouchdb-adapter-idb": "^7.0.0",
    "pouchdb-authentication": "^1.1.3",
    "pouchdb-core": "^7.0.0",
    "pouchdb-debug": "^7.0.0",
    "pouchdb-fetch": "^7.0.0",
    "pouchdb-find": "^7.0.0",
    "pouchdb-replication": "^7.0.0",
    "rxjs": "^6.3.3",
    "uid-generator": "^1.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.13.0",
    "@angular-devkit/build-angular": "^0.12.2",
    "@angular-devkit/core": "^7.2.2",
    "@angular-devkit/schematics": "^7.2.2",
    "@angular/cli": "^7.2.2",
    "@angular/compiler": "^7.2.1",
    "@angular/compiler-cli": "^7.2.1",
    "@angular/language-service": "^7.2.1",
    "@angular/pwa": "^0.12.2",
    "@ionic/angular-toolkit": "^1.2.2",
    "@types/jest": "^23.3.5",
    "@types/node": "^10.12.0",
    "@types/puppeteer": "^1.9.0",
    "codelyzer": "^4.5.0",
    "command-exists": "^1.2.8",
    "execa": "^1.0.0",
    "expect-puppeteer": "^3.4.0",
    "express": "^4.16.4",
    "fs-extra": "^7.0.0",
    "jest": "^23.6.0",
    "jest-environment-puppeteer": "^3.5.1",
    "jest-preset-angular": "^6.0.1",
    "jest-transform-stub": "^1.0.0",
    "listr": "^0.14.2",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.3",
    "puppeteer": "^1.10.0",
    "ts-node": "~7.0.1",
    "tslint": "^5.11.0",
    "typescript": "~3.1.6"
  },
  "// greenkeeper": [
    "// pouchdb is pinned because of pouchdb/pouchdb#7391 and pouchdb/pouchdb#7495"
  ],
  "greenkeeper": {
    "ignore": [
      "pouchdb-adapter-http",
      "pouchdb-ajax",
      "pouchdb-adapter-idb",
      "pouchdb-authentication",
      "pouchdb-core",
      "pouchdb-find",
      "pouchdb-replication"
    ]
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupJest.ts",
    "moduleNameMapper": {
      "^@ionic/core/loader": "<rootDir>/../../node_modules/@ionic/core/dist/esm/es5/ionic.define.js",
      "^@ionic/core/dist/ionic/svg": "<rootDir>/scripts/svg-override.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!@capacitor|@ionic|ionicons|@ngrx)"
    ],
    "transform": {
      "^.+\\.svg$": "jest-transform-stub"
    },
    "collectCoverageFrom": [
      "**/*.ts",
      "!**/*.d.ts",
      "!src/main.ts",
      "!src/jestGlobalMocks.ts",
      "!src/polyfills.ts",
      "!src/setupJest.ts",
      "!src/version.ts",
      "!src/environments/*"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "html"
    ]
  }
}
