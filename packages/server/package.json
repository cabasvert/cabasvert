{
  "name": "cabasvert-node",
  "author": "Cabas Vert",
  "version": "0.1.0",
  "description": "The server app for Cabas Vert",
  "private": true,
  "main": "dist/index.js",
  "bin": {
    "cabasvert-server": "dist/cli.js"
  },
  "files": [
    "dist",
    "public",
    "config.json"
  ],
  "scripts": {
    "clean": "rm -rf dist/ && rm -rf .nyc_output/ && rm -rf coverage/ && rm -rf logs/",
    "prestart": "npm install",
    "start": "node dist/cli",
    "prebuild": "npm run lint",
    "build": "tsc",
    "build-test": "tsc",
    "test": "nyc -r lcov -e .ts -x \"**/*.spec.ts\" jasmine-ts --config=jasmine.json \"test/**/*.spec.ts\"",
    "monitor": "snyk test",
    "lint": "tslint 'src/**/*.ts' 'test/**/*.ts'",
    "serve": "nodemon --inspect dist/cli --watch dist",
    "watch:test": "watch 'npm run test' dist --wait=0.1",
    "watch:ts": "tsc -w"
  },
  "dependencies": {
    "@types/body-parser": "^1.16.8",
    "@types/crypto-js": "^3.1.38",
    "@types/express": "^4.11.0",
    "@types/graceful-fs": "^4.1.2",
    "@types/helmet": "^0.0.37",
    "@types/morgan": "^1.7.35",
    "@types/nodemailer": "^4.3.1",
    "@types/pouchdb": "^6.3.2",
    "@types/pouchdb-find": "^6.3.2",
    "@types/winston": "^2.3.7",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "crypto-js": "^3.1.9-1",
    "express": "^4.16.2",
    "graceful-fs": "^4.1.11",
    "helmet": "^3.9.0",
    "inversify": "^4.9.0",
    "inversify-express-utils": "^5.2.0",
    "minimist": "^1.2.0",
    "morgan": "^1.9.0",
    "nodemailer": "^4.4.1",
    "pouchdb-adapter-http": "^6.4.1",
    "pouchdb-ajax": "^6.4.1",
    "pouchdb-authentication": "^1.1.0",
    "pouchdb-core": "^6.4.1",
    "pouchdb-find": "^6.4.1",
    "reflect-metadata": "^0.1.10",
    "uid-generator": "^1.0.0",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.2",
    "jasmine": "^2.8.0",
    "jasmine-spec-reporter": "^4.2.1",
    "jasmine-ts": "^0.2.1",
    "nodemon": "^1.14.3",
    "nyc": "^11.4.1",
    "pouchdb-security-helper": "^1.1.1",
    "request": "^2.83.0",
    "snyk": "^1.62.2",
    "supertest": "^3.0.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2",
    "watch": "^1.0.2"
  }
}
