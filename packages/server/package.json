{
  "name": "cabasvert-node",
  "author": "Cabas Vert",
  "version": "0.1.0",
  "description": "The server app for Cabas Vert",
  "private": true,
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist/ && rm -rf .nyc_output/ && rm -rf coverage/ && rm -rf logs/",
    "prestart": "npm install",
    "start": "node dist/index",
    "prebuild": "npm run lint",
    "build": "tsc",
    "build-test": "tsc",
    "test": "nyc -r lcov -e .ts -x \"**/*.spec.ts\" jasmine-ts --config=jasmine.json \"test/**/*.spec.ts\"",
    "monitor": "snyk test",
    "lint": "tslint 'src/**/*.ts'",
    "serve": "nodemon --inspect dist/index --watch dist",
    "watch:test": "watch 'npm run test' dist --wait=0.1",
    "watch:ts": "tsc -w"
  },
  "dependencies": {
    "@types/body-parser": "^1.16.1",
    "@types/crypto-js": "^3.1.37",
    "@types/express": "^4.0.35",
    "@types/helmet": "^0.0.34",
    "@types/morgan": "^1.7.0",
    "@types/nodemailer": "^4.3.0",
    "@types/pouchdb": "6.3.0",
    "@types/pouchdb-find": "6.3.0",
    "@types/winston": "^2.3.0",
    "body-parser": "^1.17.0",
    "crypto-js": "^3.1.9-1",
    "express": "^4.15.0",
    "helmet": "^3.5.0",
    "inversify": "^4.9.0",
    "inversify-express-utils": "^5.2.0",
    "morgan": "^1.8.0",
    "nodemailer": "^4.4.0",
    "pouchdb-adapter-http": "^6.4.1",
    "pouchdb-ajax": "^6.4.1",
    "pouchdb-authentication": "^1.0.0",
    "pouchdb-core": "^6.4.1",
    "pouchdb-find": "^6.4.1",
    "reflect-metadata": "^0.1.10",
    "uid-generator": "^1.0.0",
    "winston": "^2.3.0"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.2",
    "jasmine": "^2.8.0",
    "jasmine-spec-reporter": "^4.2.1",
    "jasmine-ts": "^0.2.1",
    "nodemon": "^1.11.0",
    "nyc": "^11.4.1",
    "pouchdb-security-helper": "^1.1.1",
    "request": "^2.83.0",
    "snyk": "^1.25.0",
    "supertest": "^3.0.0",
    "tslint": "^4.5.1",
    "typescript": "^2.6.1",
    "watch": "^1.0.2"
  }
}
