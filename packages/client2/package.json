{
  "name": "@cabasvert/client2",
  "version": "0.3.0",
  "author": "Cabas Vert",
  "description": "Cabas Vert: A vegetable basket manager app",
  "license": "GPL-3.0",
  "homepage": "https://github.com/cabasvert",
  "private": true,
  "scripts": {
    "#clean": "rm -rf coverage www artifacts",
    "start": "parcel serve src/index.html -p 8000",
    "#start": "scripts/start.js",
    "start:watch": "scripts/start.js --watch",
    "start:prod": "scripts/start.js --prod",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build": "parcel build",
    "#build": "scripts/build.js all --prod",
    "build-browser-release": "scripts/build.js browser --prod",
    "build-android-debug": "scripts/build.js android --debug --apk",
    "build-android-release": "scripts/build.js android --prod --apk",
    "build-ios-debug": "scripts/build.js ios --debug",
    "build-ios-release": "scripts/build.js ios --prod",
    "deploy-android-debug": "adb -d uninstall org.cabasvert.client ; adb -d install android/app/build/outputs/apk/debug/app-debug.apk",
    "deploy-android-release": "adb -d uninstall org.cabasvert.client ; adb -d install android/app/build/outputs/apk/release/app-release.apk",
    "lint": "ng lint",
    "license-check": "node ../../scripts/license-check.js"
  },
  "dependencies": {
    "@cabasvert/data": "^0.1.0-rc.1",
    "@capacitor/android": "^1.5.1",
    "@capacitor/cli": "^1.5.1",
    "@capacitor/core": "^1.5.1",
    "@capacitor/ios": "^1.5.1",
    "@ionic-native/core": "^5.22.0",
    "@ionic-native/secure-storage": "^5.22.0",
    "@ionic/core": "^5.0.5",
    "@ionic/react": "^5.0.5",
    "@ionic/react-hooks": "^0.0.6",
    "@ionic/react-router": "^5.0.5",
    "cordova-plugin-secure-storage": "^3.0.2",
    "i18next": "^19.3.2",
    "pouchdb-adapter-http": "^7.0.0",
    "pouchdb-adapter-idb": "^7.0.0",
    "pouchdb-authentication": "^1.1.3",
    "pouchdb-core": "^7.0.0",
    "pouchdb-debug": "^7.0.0",
    "pouchdb-fetch": "^7.0.0",
    "pouchdb-find": "^7.0.0",
    "pouchdb-replication": "^7.0.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-i18next": "^11.3.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "rxjs": "^6.5.3",
    "uid-generator": "^1.0.0"
  },
  "devDependencies": {
    "@ionic/react-test-utils": "^0.0.3",
    "@parcel/core": "nightly",
    "@parcel/transformer-typescript-tsc": "nightly",
    "@parcel/validator-typescript": "nightly",
    "@testing-library/react": "^9.5.0",
    "@types/pouchdb-core": "^7.0.5",
    "@types/pouchdb-find": "^6.3.5",
    "@types/pouchdb-replication": "^6.4.2",
    "@types/react": "^16.9.23",
    "@types/react-router": "^5.1.4",
    "@types/react-router-dom": "^5.1.3",
    "command-exists": "^1.2.8",
    "execa": "^4.0.0",
    "express": "^4.17.1",
    "listr": "^0.14.3",
    "node": "^12.16.1",
    "parcel": "nightly",
    "prettier": "^1.19.1",
    "typescript": "^3.8.3"
  },
  "#jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupJest.ts"
    ],
    "moduleNameMapper": {
      "^@ionic/core/dist/ionic/svg": "<rootDir>/scripts/svg-override.js"
    },
    "moduleFileExtensions": [
      "ts",
      "js",
      "html",
      "json"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!@capacitor|@ionic|ionicons|@ngrx)"
    ],
    "transform": {
      "^.+\\.svg$": "jest-transform-stub"
    },
    "collectCoverageFrom": [
      "**/*.ts",
      "!**/*.d.ts",
      "!src/main.ts",
      "!src/jestGlobalMocks.ts",
      "!src/polyfills.ts",
      "!src/setupJest.ts",
      "!src/version.ts",
      "!src/environments/*"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "html"
    ]
  }
}
