{
  "name": "@cabasvert/e2e",
  "version": "0.1.0-beta.0",
  "author": "Cabas Vert",
  "description": "Cabas Vert: A vegetable basket manager app",
  "license": "GPL-3.0",
  "homepage": "https://github.com/cabasvert",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "tslint --project 'tslint.json' 'src/**/*.ts'",
    "license-check": "node ../../scripts/license-check.js"
  },
  "private": true,
  "dependencies": {
  },
  "devDependencies": {
    "@types/jest": "^23.3.5",
    "@types/mailparser": "^2.0.2",
    "@types/node": "^10.12.0",
    "@types/puppeteer": "^1.9.0",
    "@types/smtp-server": "^3.3.4",
    "expect-puppeteer": "^3.4.0",
    "jest": "^23.6.0",
    "jest-environment-puppeteer": "^3.4.0",
    "jest-preset-angular": "^6.0.1",
    "mailparser": "^2.2.0",
    "prettier": "^1.14.3",
    "puppeteer": "^1.9.0",
    "smtp-server": "^3.4.1",
    "ts-node": "~7.0.1",
    "tslint": "~5.11.0",
    "typescript": "~3.1.3"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "globalSetup": "jest-environment-puppeteer/setup",
    "globalTeardown": "jest-environment-puppeteer/teardown",
    "testEnvironment": "jest-environment-puppeteer",
    "setupTestFrameworkScriptFile": "<rootDir>/src/setup.ts",
    "transform": {
      "^.+\\.(ts|js|html)$": "<rootDir>/node_modules/jest-preset-angular/preprocessor.js",
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!@capacitor|@ionic|@ngrx)"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "**/*.ts",
      "!**/*.d.ts",
      "!**/src/*"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "html",
      "json"
    ]
  }
}
