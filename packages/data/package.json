{
  "name": "@cabasvert/data",
  "author": "Cabas Vert",
  "version": "0.1.0-beta.1",
  "description": "The data models for Cabas Vert",
  "publishConfig": {
    "access": "public"
  },
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "clean": "rm -rf dist/ && rm -rf .nyc_output/ && rm -rf coverage/ && rm -rf logs/",
    "compile": "tsc",
    "copy-packages": "(cd src && cp **/package.json --parents ../dist)",
    "prebuild": "npm-run-all lint license-check",
    "build": "npm-run-all compile",
    "lint": "tslint --project 'tslint.json' 'src/**/*.ts' 'test/**/*.ts'",
    "license-check": "node ../../scripts/license-check.js"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.9",
    "jasmine": "^3.2.0",
    "jasmine-spec-reporter": "^4.2.1",
    "jasmine-ts": "^0.2.1",
    "nyc": "^11.4.1",
    "tslint": "^5.8.0",
    "typescript": "~2.9.2"
  }
}
