{
  "name": "@cabasvert/tools",
  "author": "Cabas Vert",
  "version": "0.1.1-beta.3",
  "description": "The admin and dev tools for Cabas Vert",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "bin": {
    "cvt": "cli.js"
  },
  "files": [
    "cli.js",
    "dist",
    "assets",
    "config.json"
  ],
  "scripts": {
    "clean": "rm -rf dist .nyc_output coverage logs",
    "compile": "tsc",
    "prebuild": "npm-run-all lint license-check",
    "build": "npm-run-all compile",
    "lint": "tslint --project 'tslint.json' 'src/**/*.ts'",
    "license-check": "node ../../scripts/license-check.js"
  },
  "dependencies": {
    "@cabasvert/data": "^0.1.0-beta.3",
    "@types/crypto-js": "^3.1.38",
    "@types/mz": "^0.0.32",
    "@types/pouchdb": "^6.3.2",
    "@types/pouchdb-find": "^6.3.2",
    "crypto-js": "^3.1.9-1",
    "graceful-fs": "^4.1.11",
    "inversify": "^5.0.1",
    "minimist": "^1.2.0",
    "mz": "^2.7.0",
    "pouchdb-adapter-http": "^6.4.3",
    "pouchdb-ajax": "^6.4.3",
    "pouchdb-authentication": "^1.1.3",
    "pouchdb-core": "^6.4.3",
    "pouchdb-find": "^6.4.3",
    "reflect-metadata": "^0.1.10",
    "uid-generator": "^1.0.0",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.9",
    "jasmine": "^3.2.0",
    "jasmine-spec-reporter": "^4.2.1",
    "pouchdb-security-helper": "^1.1.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.8.0",
    "typescript": "^3.1.3",
    "watch": "^1.0.2"
  },
  "// greenkeeper": [
    "// pouchdb is pinned because of pouchdb/pouchdb#7391 and pouchdb/pouchdb#7495"
  ],
  "greenkeeper": {
    "ignore": [
      "pouchdb-adapter-http",
      "pouchdb-ajax",
      "pouchdb-authentication",
      "pouchdb-core",
      "pouchdb-find",
      "pouchdb-security-helper"
    ]
  }
}
